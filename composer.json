{
	"name": "w7/rangine-cache-model",
	"description": "对Model扩展缓存支持",
	"type": "library",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "GordenSong",
			"email": "94581976@qq.com"
		}
	],
	"require": {
		"php": "7.*",
		"genealabs/laravel-model-caching": "^0.6.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.2",
		"w7/php-cs-fixer": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"W7\\CacheModel\\": "src/"
		},
		"classmap": [
			"src/Traits/Buildable.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"W7\\CacheModel\\Tests\\": "tests/"
		}
	},
	"extra": {
		"rangine": {
			"providers": [
				"W7\\CacheModel\\CacheModelProvider"
			]
		}
	}
}
