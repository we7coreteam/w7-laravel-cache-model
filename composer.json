{
	"name": "w7/rangine-cache-model",
	"description": "对Model扩展缓存支持",
	"type": "library",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "titrxw",
			"email": "1120309488@qq.com"
		}
	],
	"repositories": [
		{
			"type": "path",
			"url": "../w7-rangine"
		}
	],
	"require": {
		"w7/rangine": "dev-master",
		"genealabs/laravel-model-caching": "^0.9.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.2",
		"w7/php-cs-fixer": "*"
	},
	"autoload": {
		"psr-4": {
			"W7\\CacheModel\\": "src/"
		},
		"classmap": [
			"src/Traits/Buildable.php",
			"src/Traits/Caching.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"W7\\CacheModel\\Tests\\": "tests/"
		}
	},
	"extra": {
		"rangine": {
			"providers": [
				"W7\\CacheModel\\CacheModelProvider"
			]
		}
	}
}
